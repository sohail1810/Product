package com.sathya.springMVC3.Model;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Data
@NoArgsConstructor
@AllArgsConstructor
@Getter
@Setter
public class Model 
{
	private String name;
	private String dept;
	private String gender;
	private int age;
	private String school;

}
